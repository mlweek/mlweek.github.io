<!-- Contact us / Map -->
<div class="maps" id="location">
	<div id="js-map"></div>
	<div class="map-location">
		<h2><%= lang.contacts.location %></h2>
		<ul itemscope="" itemtype="http://schema.org/PostalAddress">
			<li class="title"><%= lang.contacts.address.title %></li>
			<li itemprop="address"><%- render(lang.contacts.address.details) %> </li>
			<li class="title"><%= lang.contacts.email.title %></li>
			<li itemprop="email"><%= lang.contacts.email.details %></li>
			<li class="title"><%= lang.contacts.phone.title %></li>
			<li itemprop="phone"><%= lang.contacts.phone.english %></li>
			<li itemprop="phone"><%= lang.contacts.phone.french %></li>
		</ul>
	</div>
</div>
