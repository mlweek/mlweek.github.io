<!-- Schedule -->
<div class="schedule widget-content" id="schedule">
	<div class="row">
		<div class="columns small-12 medium-12 large-12">
			<div class="center-content">
				<h2><%= lang.schedule.title %></h2>
				<h4 style="text-align:left"><%= lang.schedule.details %></h4>
			</div>
				<ul class="small-block-grid-1 medium-block-grid-2 large-block-grid-2">
					<!-- First day -->
					<li>
						<div class="schedule-body">
							<div class="event-header">
								<div class="day">
									<%= lang.schedule.part1.title %>
								</div>
								<div class="title">

								</div>
							</div>
							<div class="content">
								<dl class="accordion" data-accordion>

									<dd class="accordion-navigation">
									<a href="#panel1">
										<div class="event-title large-8"> <%= lang.schedule.part1.step1_title %></div>
									</a>
									<div id="panel1" class="content">
										<%= lang.schedule.part1.step1_details %>
									</div>
									</dd>

									<dd class="accordion-navigation">
									<a href="#panel2">
